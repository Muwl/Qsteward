          <text>我要申购>></text>
          <image src="../../images/purchase_1.png" class='textLine'></image>
        </view>
      </View>
    </view>
    <view class="btnClick">
        <View  class="listHref" bindtap="issueedClick"><button type="default" class='seemore' bindtap='seemore'>我的发布</button></View>
