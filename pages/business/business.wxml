      <View class="listHref" bindtap="issueClick">
        <image src="../../images/release.png" class="bgPic"></image>
        <view class="contextText">
          <text>我要发布>></text>
          <image src="../../images/release_1.png" class='textLine'></image>
        </view>
      </View>
    </view>  
    <view class='contentList'>

    <View  class="listHref" bindtap="sgClick">
        <image src="../../images/release.png" class="bgPic"></image>
