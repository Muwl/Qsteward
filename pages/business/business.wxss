.businessContent{
  margin: 30rpx 18rpx 0rpx 18rpx;
  display: flex;
  flex-direction: column
}
.contentList{
  flex: 1;
  width:714rpx;
  height: 280rpx;
  position: relative;
  margin-bottom: 10rpx
}
.contentList .bgPic{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top:0;
  z-index: 0
}
.listHref{
  width: 100%;
  height: 100%;
  overflow: hidden
}
.contextText{
  position: relative;
  z-index: 1;
  display: flex;
  flex-direction: column;
  color: #333;
  font-size: 34rpx;
  margin: 111rpx 49.5rpx 121rpx 76.5rpx
}
.textLine{
  width: 179rpx;
  height: 4rpx;
  margin-top:6rpx 
}
.btnClick{
  padding-top: 30rpx
}
.seemore{
  width: 640rpx;
  height: 100rpx;
  margin-bottom: 28rpx;
  font-size: 24rpx;
  color: #8C959F;
  line-height: 100rpx;
  background: #F4F4F4;
  border-radius: 8px;
  letter-spacing: 2px;
}