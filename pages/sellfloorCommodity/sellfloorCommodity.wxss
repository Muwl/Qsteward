.sellfloorScreen{
  display: flex;
  flex-direction: column
}
.screenList{
  display: flex;
  flex-direction: column;
  flex: 1;
  padding:40rpx 0rpx 40rpx;
  border-bottom: 1px solid #E7E7E7;
}
.screenlist-title{
  display: flex;
  flex-direction: row;
  flex: 1;
  justify-content:space-between;
  margin: 0rpx 20rpx 34rpx 40rpx;
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #000000;
  letter-spacing: 0;
}
.currentPic{
  display: flex;
  align-items:center;
  justify-content:center;
  width: 44rpx;
  height: 44rpx
}
.currentPic image{
  width: 22rpx;
  height: 14rpx
}
.itemContent{
  display: flex;
  flex-wrap:wrap;
}
.dataTime{
  display: flex;
  flex: 1;
  justify-content:center;
  align-items:center;
}
.picker{
  width: 266rpx;
  height: 42rpx;
  border: 1px solid rgba(0, 0, 0, .2);
  border-radius:24rpx;
  padding: 0rpx 8rpx;
  line-height: 42rpx;
  font-size: 24rpx;
  text-align: center
}
.screenlist-content .line{
  width: 26rpx;
  height: 2px;
  background: #7081E0;
  border-radius: 13px;
  margin:0rpx 12rpx
} 
.itemContent text{
  width: 100%;
}
.radio-group{
  display: flex;
  flex: 1;
  flex-direction: column;
  justify-content:flex-start;
  margin: 0rpx 80rpx;
}
.radio{
  width: 100%;
  margin: 20rpx 0rpx;
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #000000;
  line-height:48rpx;
}
.radio radio{
  transform:scale(.8);
  margin-right:20rpx;
}
.scrollView{
  white-space: nowrap;
  display: flex
}
.itemBox{
  display: flex;
  align-items:center;
  margin: 0rpx 20rpx 0rpx 50rpx;
  flex-wrap:wrap;
}
.scrollViewItem{
  display: inline-block;
  padding: 0rpx 26rpx;
  height: 80rpx;
  background: #F4F4F4;
  border-radius: 8px;
  text-align: center;
  line-height: 80rpx;
  margin: 0rpx 20rpx 30rpx;
}
.scrollView:last-child{
   margin: 0px 0px 0px 20rpx;
}
.money-list{
  display: flex;
  width: 100%;
  flex-direction: row;
  font-family: SCFwxz;
  font-size: 32rpx;
  color: #8C959F;
  line-height:32rpx;
  align-items:center;
  align-content:center;
  padding: 10rpx 0rpx;
}
.seletedImage{
  width:32rpx;
  height:32rpx;
  margin: 0rpx 40rpx 0rpx 80rpx;
}
.xuanzePic{
  display: flex;
  align-items:center;
}
.sellfloorScreenBtn{
  display: flex;
  flex-direction: row;
  padding:60rpx 0rpx 50rpx;
  font-family: PingFangSC-Medium;
  font-size: 24rpx;
  letter-spacing: 2px;
  line-height: 88rpx;
  justify-content:center;
}
.reset{
  width: 300rpx;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  background: #000000;
  border-radius: 100rpx;
  margin: 0rpx 20rpx;
  color: #FFFFFF;
  opacity: 0.2
}
.submitsure{
  width: 300rpx;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  background: #8392EA;
  border-radius: 100rpx;
  margin: 0rpx 20rpx;
  color: #FFFFFF;
}
.fixedwidth{
  padding:0rpx 0rpx;
  height:80rpx;
  background:#F4F4F4;
  border-radius:8px;
  text-align:center;
  line-height:80rpx;
  width: 188rpx;
  margin:0rpx 20rpx 30rpx;

}